{
  "expo": {
    "name": "Mount Zoom",
    "slug": "mount-zoom",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mountzoommobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "icon": "./assets/images/icon.png",

    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#080809"
    },

    "ios": {
      "supportsTablet": true
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
